#!/bin/bash

# TYPE: alias
# DESC: Opens new Chrome window
function chr {
	open -n /Applications/Google\ Chrome.app || open -n $APPS_DIR/Google\ Chrome.app
}

# TYPE: function
# DESC: Opens new Minecraft client
function min {
	open /Applications/Minecraft.app || open $APPS_DIR/Minecraft.app
}

# TYPE: function
# DESC: Opens System Preferences
function set {
	open -a /Applications/System\ Preferences.app
}